<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.samthompson.bubbleactions_sample" >

    <application
        android:name=".BubbleActionsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="me.samthompson.bubbleactions_sample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="me.samthompson.bubbleactions_sample.RecyclerViewActivity" >
        </activity>
        <activity android:name="me.samthompson.bubbleactions_sample.KotlinActivity" >
        </activity>
    </application>

</manifest>
